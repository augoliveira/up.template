  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "yarn prettier:check && yarn lint",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "lint": "eslint src --max-warnings=0",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write --ignore-unknown .",
